#include <iostream>
using namespace std;


auto init(int a[], int n) -> void {
    for (int i = 0; i < n; i++) {
        a[i] = 0;
    }
}


auto main (int argc, char** argv) -> int {
    int tab[100];
    int n =10;
    init(tab, n);

    for (int i = 0; i < n; ++i) {
        cout << tab[i] << endl;
    }
    return 0;
}
