#include <iostream>
using namespace std;


auto asum(int a[], int n) -> int {
    int sum = 0;
    for (int i = 0; i < n; i++) {
        sum += a[i];
    }
    return sum;
}
    auto main(int argc, char **argv) -> int {
        int tab[100];
        int n = 10;
        int a[] = {42, 9, -1, 18, 59, 3, 101, 31, 72, 12};
        cout << asum(a, n)<<"\n";
    return 0;
}
