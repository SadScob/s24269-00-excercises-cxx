#include <iostream>
using namespace std;


auto amin(int a[], int n) -> int {
    int Min = a[0];
    for (int i = 0; i < n; i++) {
        if (a[i]< Min){
            Min = a[i];
}
    }
    return Min;
}

    auto main(int argc, char **argv) -> int {
        int tab[100];
        int n = 10;
        int b[] = {42, 9, -1, 18, 59, 3, 101, 31, 72, 12};

        cout << amin(b,n) <<"\n";
    return 0;
}
